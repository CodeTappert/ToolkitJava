<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="900.0" prefWidth="1600.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="Einheitenumrechner.GeschwindigkeitController">
    <children>
        <HBox alignment="CENTER" layoutX="6.0" prefHeight="900.0" prefWidth="1600.0">
            <children>
                <VBox alignment="CENTER" prefHeight="900.0" prefWidth="600.0">
                    <children>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                            <children>
                                <Label alignment="CENTER" minHeight="50.0" minWidth="200.0" text="Eingabe:">
                                    <font>
                                        <Font size="16.0"/>
                                    </font>
                                </Label>
                                <TextField fx:id="TFStart" alignment="CENTER" minHeight="50.0" minWidth="200.0"
                                           promptText="Startgröße">
                                    <font>
                                        <Font size="16.0"/>
                                    </font>
                                </TextField>
                                <ComboBox fx:id="CBEingabe" minWidth="200.0" prefHeight="50.0" prefWidth="200.0"/>
                            </children>
                        </HBox>
                    </children>
                </VBox>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <Button fx:id="BtnCalc" minHeight="50.0" minWidth="150.0" mnemonicParsing="false"
                                onAction="#BtnCalc" text="Umrechnen">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Button>
                    </children>
                </VBox>
                <VBox fx:id="VBoxErgebniss" alignment="CENTER" layoutY="100.0" maxHeight="800.0" maxWidth="600.0"
                      prefHeight="800.0" prefWidth="600.0" spacing="15.0">
                    <children>
                        <HBox fx:id="HMeter" alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="50.0">
                            <children>
                                <Label fx:id="LMeter" alignment="CENTER" minHeight="50.0" minWidth="200.0"
                                       text="Meter pro Sekunde:">
                                    <font>
                                        <Font size="16.0"/>
                                    </font>
                                </Label>
                                <TextField fx:id="TFMeter" minHeight="50.0" minWidth="350.0"
                                           promptText="Meter pro Sekunde">
                                    <font>
                                        <Font size="16.0"/>
                                    </font>
                                </TextField>
                            </children>
                        </HBox>
                        <HBox fx:id="HFuss" alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="50.0">
                            <children>
                                <Label fx:id="LFuss" alignment="CENTER" minHeight="50.0" minWidth="200.0"
                                       text="Fuß pro Sekunde:">
                                    <font>
                                        <Font size="16.0"/>
                                    </font>
                                </Label>
                                <TextField fx:id="TFFuss" minHeight="50.0" minWidth="350.0"
                                           promptText="Fuß pro Sekunde">
                                    <font>
                                        <Font size="16.0"/>
                                    </font>
                                </TextField>
                            </children>
                        </HBox>
                        <HBox fx:id="HKilometer" alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="50.0">
                            <children>
                                <Label fx:id="LKilometer" alignment="CENTER" minHeight="50.0" minWidth="200.0"
                                       text="Kilometer pro Stunde:">
                                    <font>
                                        <Font size="16.0"/>
                                    </font>
                                </Label>
                                <TextField fx:id="TFKilometer" minHeight="50.0" minWidth="350.0"
                                           promptText="Kilometer pro Stunde">
                                    <font>
                                        <Font size="16.0"/>
                                    </font>
                                </TextField>
                            </children>
                        </HBox>
                        <HBox fx:id="HMeilen" alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="50.0">
                            <children>
                                <Label fx:id="LMeilen" alignment="CENTER" minHeight="50.0" minWidth="200.0"
                                       text="Meilen pro Stunde:">
                                    <font>
                                        <Font size="16.0"/>
                                    </font>
                                </Label>
                                <TextField fx:id="TFMeilen" minHeight="50.0" minWidth="350.0"
                                           promptText="Meilen pro Stunde">
                                    <font>
                                        <Font size="16.0"/>
                                    </font>
                                </TextField>
                            </children>
                        </HBox>
                        <HBox fx:id="HKnoten" alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="50.0">
                            <children>
                                <Label fx:id="LKnoten" alignment="CENTER" minHeight="50.0" minWidth="200.0"
                                       text="Knoten:">
                                    <font>
                                        <Font size="16.0"/>
                                    </font>
                                </Label>
                                <TextField fx:id="TFKnoten" minHeight="50.0" minWidth="350.0" promptText="Knoten">
                                    <font>
                                        <Font size="16.0"/>
                                    </font>
                                </TextField>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </HBox>
        <Button fx:id="BtnBack" layoutX="6.0" layoutY="8.0" minHeight="50.0" minWidth="200.0" mnemonicParsing="false"
                onAction="#BtnBack" text="Zurück zum Hauptmenü">
            <font>
                <Font size="17.0"/>
            </font>
        </Button>
        <HBox alignment="CENTER" layoutX="1000.0" layoutY="846.0" minHeight="50.0" minWidth="600.0" spacing="50.0"/>
    </children>
</AnchorPane>
