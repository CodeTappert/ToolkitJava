<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="900.0" maxWidth="1600.0" minHeight="900.0" minWidth="1600.0"
            prefHeight="900.0" prefWidth="1600.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="Hauptmenu.HauptmenuController">
   <children>
       <VBox alignment="CENTER" layoutX="550.0" prefHeight="900.0" prefWidth="500.0" spacing="30.0">
         <children>
             <Button fx:id="BtnEinheitenumrechner" mnemonicParsing="false" onAction="#BtnEinheitenumrechner"
                     prefHeight="50.0" prefWidth="400.0" text="Einheitenumrechner">
                 <font>
                     <Font size="20.0"/>
                 </font>
             </Button>
             <Button fx:id="BtnInternet" mnemonicParsing="false" onAction="#BtnInternet" prefHeight="50.0"
                     prefWidth="400.0" text="Internetgeschwindigkeitsrechner">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
             <Button fx:id="BtnZahlensystemumrechner" mnemonicParsing="false" onAction="#BtnZahlensystemumrechner"
                     prefHeight="50.0" prefWidth="400.0" text="Zahlensystemumrechner">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
             <Button fx:id="BtnExit" mnemonicParsing="false" onAction="#BtnExit" prefHeight="50.0" prefWidth="400.0"
                     text="Programm beenden">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
